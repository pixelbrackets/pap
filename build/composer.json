{
    "name": "pixelbrackets/deployment",
    "description": "pixelbrackets Deployment Script",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "version": "4.1.0",
    "require-dev": {
        "consolidation/robo": "^1.1.3",
        "patchwork/jsqueeze": "~1.0",
        "natxet/CssMin": "~3.0",
        "jakub-onderka/php-parallel-lint": "~0.9",
        "friendsofphp/php-cs-fixer": "~2.0",
        "editorconfig-checker/editorconfig-checker": "~6.0",
        "phploc/phploc": "~3.0"
    },
    "scripts": {
        "create-robo-symlink": [
            "ln -snf vendor/bin/robo robo.phar"
